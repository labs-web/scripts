{
    "Description":"Liste des packages avec des livrables personnalisés",
    "DefaultAuthorizedDirectories":[
        "docs/$package_name",
        "scripts",
        "$package_name"
    ],
    "LaravelAuthorizedDirectories":[
        "app/app/Exports/$package_name",
        "app/app/Imports/$package_name",
        "app/app/Http/Controllers/$package_name",
        "app/app/Http/Requests/$package_name",
        "app/app/Models/$package_name",
        "app/app/Repositories/$package_name",
        "app/resources/views/$package_name",
        "app/routes/web.php",
        "app/routes/$package_name.php",
        "app/database/factories/$package_name",
        "app/database/migrations/$package_name",
        "app/database/seeders/$package_name",
        "app/tests/Feature/$package_name",
        "app/tests/Unit/$package_name"
    ],
    "Packages" : [
        {
            "Titre":"init-maquettes",
            "Description":"init-maquettes",
            "IsValidaionAvecFormateur":true,
            "IsUseUnique":false,
            "Chemins":[
                "maquettes/"
            ]
        },
        {
            "Titre":"backlog",
            "Description":"Backlog de projet",
            "IsValidaionAvecFormateur":true,
            "IsUseUnique":false,
            "Chemins":[
                "docs/backlog/",
                "backlog/"
            ]
        },
        {
            "Titre":"init-lab",
            "Description":"Initialisation de dépôt",
            "IsUseUnique":false,
            "IsValidaionAvecFormateur":false,
            "Chemins":[
                ".github/",
                ".vscode/",
                "docs/_layouts/",
                "docs/assets/",
                "docs/_config.yml",
                "docs/.gitignore",
                "docs/Gemfile",
                "docs/Gemfile.lock",
                "docs/images/",
                "docs/index.md",
                "docs/presentation.md",
                "docs/run_jekyll.ps1",
                "README.json",
                "README.md"
            ]
        },
        {
            "Titre":"init-app",
            "Description":"Initialisation de code, Il est valide si le dossier app n'existe pas",
            "IsValidaionAvecFormateur":true,
            "IsUseUnique":true,
            "Chemins":[
                "app/"
            ]
        },
        {
            "Titre":"custom-css-rapport",
            "Description":"Curstom css rapport et présentation",
            "IsValidaionAvecFormateur":true,
            "IsUseUnique":true,
            "Chemins":[
                "docs/assets/css/custom-presentation.css",
                "docs/assets/css/custom-rapport.css"
